{
  "name": "ms-save-return",
  "version": "1.0.0",
  "description": "save and resume for hof session data",
  "main": "index.js",
  "scripts": {
    "start": "node ./lookup/app.js",
    "alerts": "node ./alerts",
    "lookup": "node ./lookup/app.js",
    "lookup-build": "cd ./lookup && hof-build watch",
    "lint": "eslint .",
    "test": "npm run migrate && npm run seed && npm run rollback",
    "migrate": "knex migrate:latest",
    "rollback": "knex migrate:rollback",
    "seed": "knex seed:run",
    "docker-postinstall": "cd ./lookup && hof-build"
  },
  "pre-commit": [
    "lint"
  ],
  "author": "Ben Marvell",
  "license": "MIT",
  "dependencies": {
    "body-parser": "^1.19.0",
    "express": "^4.17.1",
    "knex": "^0.21.2",
    "moment": "^2.27.0",
    "morgan": "^1.10.0",
    "morgan-json": "^1.1.0",
    "node-json-logger": "0.0.11",
    "notifications-node-client": "^4.8.0",
    "pg": "^8.3.0",
    "hof": "^17.0.2",
    "hof-build": "^1.3.4",
    "hof-theme-govuk": "^5.1.1",
    "jquery": "^3.5.1"
  },
  "devDependencies": {
    "eslint": "^6.7.1",
    "eslint-config-homeoffice": "^2.2.0",
    "pre-commit": "^1.2.2"
  }
}
